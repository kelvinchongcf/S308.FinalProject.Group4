<Window x:Class="FitnessClub.MainMenu"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FitnessClub"
        mc:Ignorable="d"
        Title="MainMenu" Height="300" Width="300">
    <Grid x:Name="wdwMainMenu" Background="#FF640C97">
        <Button x:Name="btnMembershipSales" Content="Membership Sales" HorizontalAlignment="Left" Margin="75,121,0,0" VerticalAlignment="Top" Width="144"/>
        <Button x:Name="btnPricingMgmt" Content="Pricing Management" HorizontalAlignment="Left" Margin="75,146,0,0" VerticalAlignment="Top" Width="144"/>
        <Button x:Name="btnMemberInfo" Content="Membership Information" HorizontalAlignment="Left" Margin="75,171,0,0" VerticalAlignment="Top" Width="144"/>
        <Button x:Name="btnExit" Content="Exit" HorizontalAlignment="Left" Margin="216,232,0,0" VerticalAlignment="Top" Width="66"/>
        <Image x:Name="image" HorizontalAlignment="Left" Height="51" Margin="53,65,0,0" VerticalAlignment="Top" Width="184" Source="Images/Power Fitness.png" Stretch="UniformToFill"/>
        <Image x:Name="image1" HorizontalAlignment="Left" Height="52" Margin="93,21,0,0" VerticalAlignment="Top" Width="100" Source="Images/PowerFitnessLogo.png" Stretch="UniformToFill"/>
    </Grid>
</Window>
